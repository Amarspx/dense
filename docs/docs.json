                                    "name": "init",
                                    "access": "",
                                    "virtual": false,
                                    "description": "Renders a pixel-ratio-aware image.\n\nThe init method is the default that is called\nif no method argument is specified.",
                                    "parameters": [
                                        {
                                            "name": "options",
                                            "type": "Object",
                                            "description": "Options",
                                            "default": "{}",
                                            "optional": true,
                                            "nullable": ""
                                        },
                                        {
                                            "name": "options.ping",
                                            "type": "Boolean",
                                            "description": "A prefix added to the generated links",
                                            "default": "true",
                                            "optional": true,
                                            "nullable": ""
                                        },
                                        {
                                            "name": "options.dimensions",
                                            "type": "String",
                                            "description": "Whether to do with the 'width' and 'height'. Either \"update\", \"remove\" or \"preserve\"",
                                            "default": "preserve",
                                            "optional": true,
                                            "nullable": ""
                                        }
                                    ],
                                    "examples": [
                                        "$('img.retina').dense({\n 'ping'      : false,\n 'dimension' : true\n});"
